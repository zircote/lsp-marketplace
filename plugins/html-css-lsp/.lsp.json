{
    "html": {
        "command": "vscode-html-language-server",
        "args": ["--stdio"],
        "extensionToLanguage": {
            ".html": "html",
            ".htm": "html"
        },
        "transport": "stdio",
        "initializationOptions": {},
        "settings": {},
        "maxRestarts": 3
    },
    "css": {
        "command": "vscode-css-language-server",
        "args": ["--stdio"],
        "extensionToLanguage": {
            ".css": "css",
            ".scss": "scss",
            ".less": "less"
        },
        "transport": "stdio",
        "initializationOptions": {},
        "settings": {},
        "maxRestarts": 3
    }
}
